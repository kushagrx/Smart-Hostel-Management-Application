{
    "name": "smartstay-backend",
    "version": "1.0.0",
    "description": "SmartStay Management Backend",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "db:backup": "ts-node src/scripts/backup/backup.ts",
        "db:restore": "ts-node src/scripts/backup/restore.ts",
        "db:restore:list": "ts-node src/scripts/backup/restore.ts --list",
        "db:cleanup": "ts-node src/scripts/backup/cleanup.ts",
        "db:backup:schedule": "ts-node src/scripts/backup/scheduler.ts"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "exceljs": "^4.4.0",
        "expo-server-sdk": "^6.0.0",
        "express": "^4.18.2",
        "firebase-admin": "^13.6.1",
        "google-auth-library": "^8.7.0",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "multer": "^2.0.2",
        "node-cron": "^3.0.3",
        "pg": "^8.9.0",
        "puppeteer": "^21.11.0",
        "razorpay": "^2.9.6"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/multer": "^2.0.0",
        "@types/node": "^18.14.0",
        "@types/node-cron": "^3.0.11",
        "@types/pg": "^8.6.6",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    }
}
